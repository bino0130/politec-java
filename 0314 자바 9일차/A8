package chap01;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.HashSet;
import java.util.InputMismatchException;
import java.util.List;
import java.util.Scanner;

public class Chap01 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		List<String> list1 = new ArrayList<String>();
		List<String> list2 = new ArrayList<String>();
		HashSet<String> hash1 = new HashSet<String>();
		HashSet<String> hash2 = new HashSet<String>();

		int cnt = 0;

		try {
			String a = sc.next();
			String b = sc.next();
			
			String[] arr1 = a.split("");
			for (int i = 0; i < arr1.length; i++) {
				list1.add(arr1[i]);
				hash1.add(arr1[i]);
			}
			
			String[] arr2 = b.split("");
			for (int i = 0; i < arr2.length; i++) {
				list2.add(arr2[i]);
				hash2.add(arr2[i]);
			}
			
		} catch (InputMismatchException e) {
			System.out.println("InputMismatchException error");
		} catch (ArrayIndexOutOfBoundsException e) {
			System.out.println("ArrayIndexOutOfBoundsException error");
		} catch (NullPointerException e) {
			System.out.println("NullPointerException error");
		}
			
		for (int i = 0; i < hash1.size(); i++) {
			if (hash1.contains(list2.get(i))) {
				cnt++;
			}
		}
		
		System.out.println("1 : " + list1.size());
		System.out.println("2 : " + list2.size());
		System.out.println("3 : " + cnt);
	}

}
