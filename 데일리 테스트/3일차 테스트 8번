package another;

import java.util.Scanner;

// 3일차 데일리 테스트 문제 8번
// 구구단 (n X 2)부터 (n X 5) 까지만 출력
public class Practice {

	public static void main(String[] args) {
		for (int j = 2; j < 10; j++) {
			for (int i = 2; i < 6; i++) {
				System.out.print(i + " X " + j + " = " + i * j + " ");
			}
			System.out.println();
		}
	}
	
}
